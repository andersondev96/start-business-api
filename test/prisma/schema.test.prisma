generator client {
    provider = "prisma-client-js"
    output   = "../../src/database/test-client"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL_TEST")
}

model User {
    id        String   @id @default(uuid())
    name      String
    email     String   @unique
    password  String
    avatar    String?
    favorites String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
